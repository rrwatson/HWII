#include <iostream>
using namespace std;
template <class T>
T multPoly (T a, T b, T c, T d, T e, T f){
    T answerMulti;
    T answerAdd;
    T x = 4;
    answerMulti = (a*(x*x)+b*x+c)*(d*(x*x)+e*x+f);
    return (answerMulti);
    return (answerAdd);

}
template<class Y>
Y addPoly (Y a, Y b, Y c, Y d, Y e, Y f){
    Y answerAdd;
    Y x = 4;
    answerAdd = (a*(x*x)+b*x+c)+(d*(x*x)+e*x+f);
    return (answerAdd);

}
int main(){
int a=2, b=3, c=4, d=1, e=2, f=12, g, h;
g=multPoly<float>(a,b,c,d,e,f);
h=addPoly<float>(a,b,c,d,e,f);
cout<<"Answer for multiplication is: "<<g<<"\n";
cout<<"Answer for addition is: "<<h;
}
